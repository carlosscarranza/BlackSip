{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "gijgo"
    ]
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts",
    "node_modules/gijgo-angular-wrappers/index.ts"
  ],
  "include": [
    "src/**/*.ts",
    "node_modules/gijgo-angular-wrappers/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts",
  ]
}
